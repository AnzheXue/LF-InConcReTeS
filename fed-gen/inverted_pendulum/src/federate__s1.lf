@_fed_config()
main reactor  {

    s1 = new Sensor() at 10.34.167.170;
    
    
    
    
    
    @_tpoLevel(13)
    ns_federate__p1 = new NetworkSender_0<{= 0 =}>()
    @_tpoLevel(15)
    ns_federate__p1_0 = new NetworkSender_1<{= 1 =}>()
    @_tpoLevel(17)
    ns_federate__p1_1 = new NetworkSender_2<{= 2 =}>()
    @_tpoLevel(19)
    ns_federate__p1_2 = new NetworkSender_3<{= 3 =}>()
    @_tpoLevel(1)
    ns_federate__plant = new NetworkSender_4<{= 4 =}>()
    @_tpoLevel(1)
    ns_federate__p1_3 = new NetworkSender_5<{= 5 =}>()
    @_tpoLevel(4)
    nr_federate__s1 = new NetworkReceiver_140()
    @_tpoLevel(6)
    nr_federate__s1_0 = new NetworkReceiver_141()
    @_tpoLevel(8)
    nr_federate__s1_1 = new NetworkReceiver_142()
    @_tpoLevel(10)
    nr_federate__s1_2 = new NetworkReceiver_143()
    _s1AP_in0 = new _s1AP_in()
    _s1AV_in0 = new _s1AV_in()
    _s1CP_in0 = new _s1CP_in()
    _s1CV_in0 = new _s1CV_in()
    _s1AP_in0.port -> s1.AP_in
    _s1AV_in0.port -> s1.AV_in
    _s1CP_in0.port -> s1.CP_in
    _s1CV_in0.port -> s1.CV_in
    s1.AP_out -> ns_federate__p1.msg
    s1.AV_out -> ns_federate__p1_0.msg
    s1.CP_out -> ns_federate__p1_1.msg
    s1.CV_out -> ns_federate__p1_2.msg
    s1.request -> ns_federate__plant.msg
    s1.sim_start_time -> ns_federate__p1_3.msg
    nr_federate__s1.msg -> _s1AP_in0.port0
    nr_federate__s1_0.msg -> _s1AV_in0.port0
    nr_federate__s1_1.msg -> _s1CP_in0.port0
    nr_federate__s1_2.msg -> _s1CV_in0.port0
}
